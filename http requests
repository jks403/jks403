import express from "express";
const app = express();
const port = 3000;
app.get("/", (req, res) => {
    res.send("<h1> Hello Joshua Good Morning </h1>");
});

app.get("/about", (req, res) => {
    res.send("<h1> I'm a computer scientist </h1>");
    console.log("hello from /about");
});

app.post("/register", (req, res)  => {
    res.sendStatus(200);
});

// Taskkill /IM node.exe /F
app.listen(port, () => {
    res.sendStatus(200);
});
